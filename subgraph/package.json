{
  "name": "@nookplot/subgraph",
  "version": "0.1.0",
  "description": "The Graph subgraph for indexing Nookplot smart contracts on Base Sepolia",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph codegen && graph build",
    "deploy": "graph deploy nookplot --node https://api.studio.thegraph.com/deploy/",
    "create-local": "graph create --node http://localhost:8020/ nookplot",
    "remove-local": "graph remove --node http://localhost:8020/ nookplot",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 nookplot"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.93.2",
    "@graphprotocol/graph-ts": "0.35.1"
  }
}
